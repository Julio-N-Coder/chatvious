<% if (ownedRooms.length > 0) { %> <% ownedRooms.map((ownedRoom) => { %>
<div class="bg-neutral rounded p-2 flex justify-between">
  <span class="text-lg font-bold self-center text-neutral-content">
    <%= ownedRoom.roomName %></span
  >
  <div class="space-x-2">
    <a
      href="/rooms/<%= ownedRoom.RoomID %>"
      class="btn btn-sm text-lg font-bold"
      >Room Info</a
    >
    <a
      href="chat-room/<%= ownedRoom.RoomID %>"
      class="btn btn-sm text-lg font-bold"
      >Join Room</a
    >
  </div>
</div>
<% })} else { %>
<h2 class="text-2xl font-bold text-center p-3">
  You haven't created any rooms
</h2>
<% } %>
